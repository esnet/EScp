/* To recompile:

  flatc -r -b session_init.fbs
  mv session_init_generated.rs ../src/session_init.rs

  flatc cli should match version from crate
*/

table Session_Init {
  version_major:   int32;
  version_minor:   int32;
  session_id:     uint64;
  crypto_key:     [byte];
  do_hash:          bool;
  do_crypto:        bool;
  do_verbose:       bool;
  no_direct:        bool;
  is_error:         bool;
  message:        string;
  bind_interface: string;
  port_start:      int32;
  port_end:        int32;
  io_engine:       int32;
  thread_count:    int32;
  block_sz:        int32;
}

